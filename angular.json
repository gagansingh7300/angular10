      "sourceRoot": "core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "core/tsconfig.lib.json",
            "project": "core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "core/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
